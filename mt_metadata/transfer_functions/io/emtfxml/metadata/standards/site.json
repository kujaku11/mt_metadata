{
    "project": {
        "type": "string",
        "required": true,
        "style": "alpha numeric",
        "units": null,
        "description": "Name of the project",
        "options": [],
        "alias": [],
        "example": "USMTArray",
		"default": null
    },
    "survey": {
      "type": "string",
      "required": true,
      "style": "free form",
      "units": null,
      "description": "Name of the survey",
      "options": [],
      "alias": [],
      "example": "MT 2020",
  		"default": null
    },
    "year_collected": {
        "type": "integer",
        "required": true,
        "style": "number",
        "units": null,
        "description": "Year data collected",
        "options": [],
        "alias": [],
        "example": "2020",
		"default": "1980"
    },
    "country": {
        "type": "string",
        "required": true,
        "style": "free form",
        "units": null,
        "description": "Country where data was collected",
        "options": [],
        "alias": [],
        "example": "USA",
		"default": null
    },
    "id": {
        "type": "string",
        "required": true,
        "style": "alpha numeric",
        "units": null,
        "description": "Station ID name.  This should be an alpha numeric name that is typically 5-6 characters long.  Commonly the project name in 2 or 3 letters and the station number.",
        "options": [],
        "alias": [],
        "example": "MT001",
		"default": null
    },
    "name": {
        "type": "string",
        "required": true,
        "style": "free form",
        "units": null,
        "description": "closest geographic name to the station",
        "options": [],
        "alias": [],
        "example": "\"Whitehorse, YK\"",
		"default": null
    },
    "acquired_by": {
        "type": "string",
        "required": true,
        "style": "free form",
        "units": null,
        "description": "Person or group who collected the data",
        "options": [],
        "alias": [],
        "example": "MT Group",
		"default": null
    },
    "start": {
        "type": "string",
        "required": true,
        "style": "date time",
        "units": null,
        "description": "Date time when the data collection started",
        "options": [],
        "alias": [],
        "example": "2020-01-01T12:00:00",
		"default": "1980-01-01T00:00:00"
    },
    "end": {
        "type": "string",
        "required": true,
        "style": "date time",
        "units": null,
        "description": "Date time when the data collection ended",
        "options": [],
        "alias": [],
        "example": "2020-05-01T12:00:00",
		"default": "1980-01-01T00:00:00"
    },
    "run_list": {
        "type": "string",
        "required": true,
        "style": "name list",
        "units": null,
        "description": "list of runs recorded by the station. Should be a summary of all runss recorded",
        "options": [
            "a",
            "b",
            "c",
            "other"
        ],
        "alias": [],
        "example": "\"[ mt001a, mt001b, mt001c ]\"",
		"default": []
    }
